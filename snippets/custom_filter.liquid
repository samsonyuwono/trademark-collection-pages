{% comment %}CRIBS FILTER{% endcomment %}
{% if collection.handle == 'cribs'%}
{% assign collection = collections[{{collection.handle}}] %}
  <form class="js-collection-filter" id="Type" data-collection="{{collection.id}}">
   <input id="hideThisBox" data-group="{{collection.handle}}" type="checkbox" disabled="" selected="" value=""></input>
   <h6>Brand:<span class="angle-down"></span></h6>
   <div id="filterList" class="fadeIn">
     {% for tag in collection.all_tags %}
       {% if tag != '' and tag == 'Delta Children' or tag =="Serta" or tag == "Simmons Kids" %}
        <ul>
         <li><input class="checkBox" data-group="{{collection.handle}}" name="brand"  type="checkbox" value="{{ tag }}"><div class="checkBoxText"> {{ tag }}</div></input></li>
        </ul>
       {% endif %}
     {% endfor %}
   </div>
  </form>

  <form class="js-collection-filter" id="Feature" data-collection="{{collection.id}}">
    <input id="hideThisBox" data-group="{{collection.handle}}" type="checkbox" disabled="" selected="" value=""></input>
    <h6>Feature:<span class="angle-down"></span></h6>
    <div id="filterList" class="fadeIn">
      {% for tag in collection.all_tags %}
        {% if tag != '' and tag == '3-in-1' or tag == '4-in-1' or tag == "Crib 'N' Changer" or tag == 'Mini Crib' or tag == 'Portable Crib' %}
        <ul>
          <li><input class="checkBox" data-group="{{collection.handle}}" name="feature" type="checkbox" value="{{ tag }}"><div class="checkBoxText"> {{ tag }}</div></input></li>
        </ul>
        {% endif %}
      {% endfor %}
    </div>
  </form>
  <form class="js-collection-filter" id="Price" data-collection="{{collection.id}}">
    <input id="hideThisBox" data-group="{{collection.handle}}" type="checkbox" disabled="" selected="" value=""></input>
    <h6>Price:<span class="angle-down"></span></h6>
    <div id="filterList" class="fadeIn">
      {% for tag in collection.all_tags %}
        {% if tag != '' and tag == '$0 - $149' or tag == '$150 - $299' or tag == '$300 - $499' or tag == '$500+' %}
        <ul>
          <li><input class="checkBox" data-group="{{collection.handle}}" name="price" type="checkbox" value="{{ tag }}"><div class="checkBoxText"> {{ tag }}</div></input></li>
        </ul>
        {% endif %}
      {% endfor %}
    </div>
  </form>
{% endif %}
