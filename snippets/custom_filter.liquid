<script>

 // define filterList globally to a string to account for undefined error thrown on pages with no filters
var filterList = "";

 // funtionality for filter Titles triggering dropdown menu
$(document).ready(function() {

  var h6 = $('h6');

  for (var i = 0; i < h6.length; i++) {

    var click = h6[i]
//for dropdown menu
    $(click).click(function(e) {
      e.stopPropagation();

      var h6Parent = $(this).parent();

      filterList = $('#filterList', h6Parent);
      var fadeIn = $('.fadeIn', h6Parent);

      if ( $(filterList).css('display') == 'none' ) {
         $(fadeIn).css('display','block');
      } else  {
         $(filterList).css('display','none');
      }

    });

  }


     // if click anywhere on page but filters or sort, close filter list (if open)
  $('body').click(function(evt){

    if (!$(evt.target).is('.checkBox') && !$(evt.target).is('#sort-by') && !$(evt.target).is('.checkBoxText')) {

      $('.fadeIn').css('display', 'none');

    }

  });

});

</script>
